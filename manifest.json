{
  "manifest_version": 2,
  "name"            : "Toss-up Project",
  "description"     : "Tournament TossUp Application",
  "short_name"      : "Toss-up app",
  "version"         : "0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon" : "images/logo.png"
  },
  "permissions": [
    "debugger",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
} 
